#: src/Dialogs/ConfigPanels/WaypointDisplayConfigPanel.cpp:171
#, no-c-format
msgid "Traffic lights"
msgstr "Verkehrsampel"

#: src/Dialogs/ConfigPanels/WaypointDisplayConfigPanel.cpp:172
#, no-c-format
